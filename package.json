{
  "name": "@fe6/norm",
  "description": "fe6 的代码美化",
  "version": "3.5.1",
  "main": "index.js",
  "scripts": {
    "jslint": "eslint . --ext '.js,.jsx,.ts,.tsx' --fix",
    "stylelint": "stylelint ./demo/**/*.scss --fix",
    "lint": "yarn jslint && yarn stylelint",
    "cz": "cz"
  },
  "engines": {
    "node": ">= 16"
  },
  "author": "李梦龙 <649076408@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fe6/norm/issues"
  },
  "homepage": "https://github.com/fe6/norm#readme",
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@fe6/norm-git-cz-config": "workspace:^",
    "@fe6/norm-prettier": "workspace:^",
    "conventional-changelog-cli": "^2.1.1",
    "cz-customizable": "^6.3.0",
    "husky": "^7.0.2",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "packageManager": "pnpm@6.32.0",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": "./packages/git-cz-config/customizable.js"
    }
  },
  "pnpm": {
    "overrides": {
      "@fe6/norm-prettier": "workspace:^",
      "@fe6/norm-git-cz-config": "workspace:^"
    }
  }
}
