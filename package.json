    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "peerDependencies": {
    "eslint": "^7.32.0",
    "prettier": "^2.2.1",
    "stylelint": "^13.9.0"
  },
  "devDependencies": {
    "husky": "^4.0.6"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    },
    "cz-customizable": {
