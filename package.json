    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-redux-saga": "^1.2.1",
    "eslint-plugin-unicorn": "^36.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "object.assign": "^4.1.0",
    "object.entries": "^1.1.1",
